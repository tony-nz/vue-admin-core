{
  "name": "@tony-nz/vue-admin-core",
  "author": "Tony Myers",
  "private": false,
  "version": "0.6.9",
  "type": "module",
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "files": [
    "/dist",
    "/src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "build:styles": "postcss src/assets/style.css -o dist/style.css",
    "preview": "vite preview"
  },
  "dependencies": {
    "i18n": "^0.15.1",
    "laravel-echo": "^1.15.3",
    "pusher-js": "^8.3.0",
    "rollup-plugin-copy": "^3.4.0",
    "vite-plugin-dts": "^2.0.2"
  },
  "devDependencies": {
    "@types/webpack-env": "^1.18.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "autoprefixer": "^10.4.13",
    "axios": "^1.3.4",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "quill": "^1.3.7",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vue": "^3.2.47",
    "vue-axios": "^3.5.2",
    "vue-i18n": "^9.2.2",
    "vue-inline-svg": "^3.1.2",
    "vue-loading-overlay": "^6.0.3",
    "vue-router": "^4.1.6",
    "vue-tsc": "^1.0.24"
  },
  "peerDependencies": {
    "chart.js": "^4.2.0",
    "deepmerge": "^4.3.0",
    "nprogress": "^0.2.0",
    "object-path": "^0.11.8",
    "pinia": "^2.1.7",
    "primevue": "^3.46.0",
    "sass": "^1.58.3",
    "vue3-toastify": "^0.1.6"
  }
